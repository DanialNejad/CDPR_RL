<mujoco model="Kamal">
    <option gravity="0 0 -9.81"/>

    <visual>
        <headlight diffuse="0.6 0.6 0.6" ambient="0.3 0.3 0.3" specular="0 0 0"/>
        <rgba haze="0.15 0.25 0.35 1"/>
        <global azimuth="-120" elevation="-20"/>
    </visual>

    <asset>
        <texture type="skybox" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="512" height="3072"/>
        <texture type="2d" name="groundplane" builtin="checker" mark="edge" rgb1="0.2 0.3 0.4" rgb2="0.1 0.2 0.3"
        markrgb="0.8 0.8 0.8" width="300" height="300"/>
        <material name="groundplane" texture="groundplane" texuniform="true" texrepeat="5 5" reflectance="0.1"/>
    </asset>

    <worldbody>

        <light pos="0 0 1.5" dir="0 0 -1" directional="true"/>
        <geom name="floor" size="0 0 0.05" type="plane" material="groundplane"/>

        <body>
            <geom name="board" type="box" size="1.5 0.005 1.1" pos="0 -0.005 1.1" rgba="1 1 1 1"/>
            <site name="r1" pos="-0.985 0.03 1.965"/>
            <site name="r2" pos="-0.985 0.03 0.235"/>
            <site name="u1" pos="0.95 0.03 2"/>
            <site name="u2" pos="-0.95 0.03 2"/>
            <site name="l1" pos="0.985 0.03 1.965"/>
            <site name="l2" pos="0.985 0.03 0.235"/>
            
            <!-- <body pos="0.967 0.02 0.217">
                <geom name="left_pulley" type="cylinder" size="0.025 0.02" euler="90 0 0"/>
                <site name="l2" pos="0.03 0.01 -0.03"/>
            </body> -->

        </body>

        <body name="ee" pos="0 0.01 1.1">
            <freejoint/>
            <geom type="cylinder" size="0.07 0.01" euler="90 0 0" rgba="0.5 0.5 0.5 1"/>

            <body name="pulley1" pos="0 0.02 0.05">
                <!-- <joint name="j1" type="hinge"/> -->
                <geom name="p1" type="cylinder" size="0.01 0.01" euler="90 0 0" rgba="0.7 0.1 0.1 1"/>
                <body>
                    <site name="side1" pos="0 0 -0.02"/>
                </body>
            </body>

            <body name="pulley2" pos="0.05 0.02 0">
                <geom name="p2" type="cylinder" size="0.01 0.01" euler="90 0 0" rgba="0.7 0.1 0.1 1"/>
                <body>
                    <site name="side2" pos="-0.02 0 0"/>
                </body>
            </body>

            <body name="pulley3" pos="-0.05 0.02 0">
                <geom name="p3" type="cylinder" size="0.01 0.01" euler="90 0 0" rgba="0.7 0.1 0.1 1"/>
                <body>
                    <site name="side3" pos="0.02 0 0"/>
                </body>
            </body>

            <body pos="0 0.04 0">
                <geom type="cylinder" size="0.07 0.01" euler="90 0 0" rgba="0.5 0.5 0.5 1"/>
            </body>

            <!-- <body name="end" pos="0 0.02 -0.04">
                <geom name="p4" type="cylinder" size="0.01 0.01" euler="90 0 0" rgba="0.2 0.2 0.4 1"/>
                <body>
                    <site name="end_point" pos="0 0 0"/>
                </body>
            </body> -->

        </body>

    </worldbody>

    <tendon>
        <spatial name="tendon1" width=".002" damping="0.1" stiffness="1">
            <site site="u1"/>
            <geom geom="p1" sidesite="side1"/>
            <site site="u2"/>
        </spatial>
    </tendon>

    <tendon>
        <spatial name="tendon2" width=".002" damping="10" stiffness="100">
            <site site="l1"/>
            <geom geom="p2" sidesite="side2"/>
            <site site="l2"/>
        </spatial>
    </tendon>

    <tendon>
        <spatial name="tendon3" width=".002" damping="10" stiffness="100">
            <site site="r1"/>
            <geom geom="p3" sidesite="side3"/>
            <site site="r2"/>
        </spatial>
    </tendon>

    <actuator>
        <motor name="tendon1" tendon="tendon1" ctrlrange="-200 200"/>
    </actuator>
</mujoco>